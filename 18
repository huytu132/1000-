int Factorial(int n){
	if(n==1)return 1;
	else 
	return n*Factorial(n-1);
}

int POW(int x,int n){
	if(n==1)return x;
	else
	return x*POW(x,n-1);
}

double Sum_Of_S(int x,int n){
	if(n==0)return 1;
	else 
	return POW(x,2*n)*1.0/Factorial(2*n)+Sum_Of_S(x,n-1);
}
//Calculate sum of S=1+x^2/2!+x^4/4!+...+x^2n/(2n)!
